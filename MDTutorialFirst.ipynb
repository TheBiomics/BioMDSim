"""## Step 1: Fetching and processing files"""

# Import Protein if doesn't exist already!

protein_file_name = '1aki.pdb'
if path.exists(protein_file_name):
  print(protein_file_name + " is already imported.")
else:
  print("Import a Protein from rcsb.org")
  pdb_url = 'http://files.rcsb.org/download/'
  protein_url = pdb_url + protein_file_name
  print("Downloading from " + protein_url)
  urllib.request.urlretrieve(protein_url, protein_file_name)
  if path.exists(protein_file_name):
    print(protein_file_name + " is successfully imported.")
  else:
    print(protein_file_name + " could not be retrieved.")

if path.exists('1AKI_clean.pdb'):
  print('1AKI_clean.pdb already exists')
else:
  print('Remove Water Molecules from fetched file')
  !grep -v HOH 1aki.pdb > 1AKI_clean.pdb

print('Convert to .gro and add force field to the topology')
if path.exists('1AKI_processed.gro'):
  print("1AKI_processed.gro already exists.")
else:
  !gmx pdb2gmx -f 1AKI_clean.pdb -o 1AKI_processed.gro -water spce -ff oplsaa
  print("You have now generated three new files: 1AKI_processed.gro, topol.top, and posre.itp. 1AKI_processed.gro is a GROMACS-formatted structure file that contains all the atoms defined within the force field (i.e., H atoms have been added to the amino acids in the protein). ")

print("Generate Box")
if path.exists('1AKI_newbox.gro'):
  print("1AKI_newbox.gro already exists.")
else:
  !gmx editconf -f 1AKI_processed.gro -o 1AKI_newbox.gro -c -d 1.0 -bt cubic

print("Add Solvent to the protein")
if path.exists('1AKI_solv.gro'):
  print("1AKI_solv.gro already exists.")
else:
  !gmx solvate -cp 1AKI_newbox.gro -cs spc216.gro -o 1AKI_solv.gro -p topol.top

print("Download Ions Parameters File for OPLA-AA force field")
if path.exists('ions.mdp'):
  print('ions.mdp already exists')
else:
  urllib.request.urlretrieve('http://www.mdtutorials.com/gmx/lysozyme/Files/ions.mdp', 'ions.mdp')

print("Create ions.tpr")
if path.exists('ions.tpr'):
  print('ions.tpr already exists')
else:
  !gmx grompp -f ions.mdp -c 1AKI_solv.gro -p topol.top -o ions.tpr

print("Generate Ions")
if path.exists('1AKI_solv_ions.gro'):
  print('1AKI_solv_ions.gro already exists')
else:
  !gmx genion -s ions.tpr -o 1AKI_solv_ions.gro -p topol.top -pname NA -nname CL -neutral

print("Energy Minisation Parameters file")
if path.exists('minim.mdp'):
  print('minim.mdp already exists')
else:
  print("Download Minimisation Parameters File for OPLA-AA force field")
  urllib.request.urlretrieve('http://www.mdtutorials.com/gmx/lysozyme/Files/minim.mdp', 'minim.mdp')

print("Energy Minisation Step")
if path.exists('em.tpr'):
  print('em.tpr already exists')
else:
  !gmx grompp -f minim.mdp -c 1AKI_solv_ions.gro -p topol.top -o em.tpr

print("Run Energy Minimisation")
!gmx mdrun -v -deffnm em -nb gpu -v

print("Export Energy Plot add 16 0")
!gmx energy -f em.edr -o potential.xvg

if path.exists('nvt.mdp'):
  print('file already downloaded')
else:
  print("Download NVT Parameters File Equilibration")
  urllib.request.urlretrieve('http://www.mdtutorials.com/gmx/lysozyme/Files/nvt.mdp', 'nvt.mdp')

print("Run NVT ensemble (constant Number of particles, Volume, and Temperature) isothermic-isochoric")

if path.exists('nvt.tpr'):
  print("nvt.tpr already exists")
else:
  !gmx grompp -f nvt.mdp -c em.gro -r em.gro -p topol.top -o nvt.tpr

print("Run mdrun with nvt")
if path.exists('nvt.trr'):
  print("nvt.trr/nvt.edr/nvt.log already exists")
else:
  # Without GPU
  # !gmx mdrun -deffnm nvt
  # With GPU
  !gmx mdrun -deffnm nvt -nb gpu  -v


print("Temperature progression graph type 18 0")
print("Run mdrun with nvt")
if path.exists('temperature.xvg'):
  print("temperature.xvg already exists")
else:
  !gmx energy -f nvt.edr -o temperature.xvg

print("Download npt Parameters File Equilibration")
if path.exists('npt.mdp'):
  print('file already downloaded')
else:
  print("Download npt Parameters File Equilibration")
  urllib.request.urlretrieve('http://www.mdtutorials.com/gmx/lysozyme/Files/npt.mdp', 'npt.mdp')

if path.exists('npt.tpr'):
  print("npt.tpr already exists")
else:
  !gmx grompp -f npt.mdp -c nvt.gro -r nvt.gro -t nvt.cpt -p topol.top -o npt.tpr

print("Job started at")
print("Run mdrun with npt")
if path.exists('npt.trr'):
  print("npt.trr already exists")
else:
  # Without GPU
  !gmx mdrun -deffnm nvt
  # With GPU
  # !gmx mdrun -deffnm npt -nb gpu  -v

print("Pressure progression graph type 24 0")
!gmx energy -f npt.edr -o pressure.xvg

print("MD Parameters File Equilibration")
if path.exists('md.mdp'):
  print("md.mdp already exists")
else:
  urllib.request.urlretrieve('http://www.mdtutorials.com/gmx/lysozyme/Files/md.mdp', 'md.mdp')

print("MD File Production for Simulation")
if path.exists('md_0_1.tpr'):
  print("md_0_1.tpr already exists. Skipping Generation")
else:
  !gmx grompp -f md.mdp -c npt.gro -t npt.cpt -p topol.top -o md_0_1.tpr

"""## Final Simulation Run"""

print("Run without gpu")
!gmx mdrun -deffnm md_0_1


